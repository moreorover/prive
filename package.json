{
	"name": "prive",
	"version": "0.0.3",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "pnpm test:integration && pnpm test:unit",
		"changeset": "changeset",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "env-cmd -f .env.local playwright test",
		"test:integration:ui": "playwright test --ui",
		"test:unit": "vitest",
		"gen:types": "npx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
		"seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
		"stripe:webhooks": "stripe listen --forward-to localhost:5173/api/stripe/webhook",
		"supabase:status": "npx supabase status -o env",
		"reset:local": "npx supabase db reset --debug && npm run seed && npm run gen:types"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.1",
		"@faker-js/faker": "^8.3.1",
		"@playwright/test": "^1.28.1",
		"@skeletonlabs/tw-plugin": "0.1.0",
		"@supabase/auth-helpers-sveltekit": "^0.9.4",
		"@supabase/supabase-js": "^2.33.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.14",
		"detect-port": "^1.5.1",
		"dotenv": "^16.3.1",
		"env-cmd": "^10.1.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"pg": "^8.11.3",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"stripe": "^13.4.0",
		"supabase": "^1.110.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"tsx": "^4.7.0",
		"typescript": "^5.0.0",
		"vite": "^4.4.12",
		"vitest": "^0.32.2",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@internationalized/date": "^3.5.0",
		"bits-ui": "^0.9.9",
		"clsx": "^2.0.0",
		"cmdk-sv": "^0.0.5",
		"formsnap": "^0.4.1",
		"lucide-svelte": "^0.291.0",
		"sveltekit-superforms": "^1.12.0",
		"tailwind-merge": "^2.0.0",
		"tailwind-variants": "^0.1.18"
	}
}
