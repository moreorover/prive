{
	"name": "prive",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts && playwright test && tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"gen:types": "npx supabase gen types typescript --local > src/lib/supabase-types.ts && prettier --write src/lib/supabase-types.ts",
		"seed": "tsx --tsconfig scripts/tsconfig.json scripts/seed.ts",
		"stripe:webhooks": "stripe listen --forward-to localhost:5173/api/stripe/webhook",
		"supabase:status": "npx supabase status -o env"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.0.2",
		"@playwright/test": "^1.28.1",
		"@skeletonlabs/skeleton": "2.0.0",
		"@skeletonlabs/tw-plugin": "0.1.0",
		"@supabase/auth-helpers-sveltekit": "^0.10.2",
		"@supabase/supabase-js": "^2.33.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/forms": "0.5.6",
		"@tailwindcss/typography": "0.5.9",
		"@types/node": "20.5.8",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "10.4.15",
		"detect-port": "^1.5.1",
		"dotenv": "^16.3.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"pg": "^8.11.3",
		"postcss": "8.4.29",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"stripe": "^13.4.0",
		"supabase": "^1.88.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^1.6.1",
		"tailwindcss": "3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-plugin-tailwind-purgecss": "0.1.3",
		"vitest": "^0.32.2",
		"zod": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"tsx": "^3.12.10"
	}
}
